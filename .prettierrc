{
    "singleQuote": true,
    "trailingComma": "all",
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "endOfLine": "lf",
    "plugins": [
        "@trivago/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
        "jsx",
        "typescript",
        "classProperties",
        "[\"decorators-legacy\", { \"decoratorsBeforeExport\": true }]"
    ]
}